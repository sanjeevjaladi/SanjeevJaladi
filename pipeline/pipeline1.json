{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Lookup1",
				"type": "Lookup",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": {
							"value": "DELETE @{pipeline().parameters.TABLE_NAME} WHERE CONVERT(DATE, @{pipeline().parameters.DATE_COLUMN_NAME} ) >= CONVERT(DATE, '@{pipeline().parameters.DATE_VALUE}')",
							"type": "Expression"
						}
					},
					"dataset": {
						"referenceName": "DS_SQL_DXC",
						"type": "DatasetReference"
					}
				}
			}
		],
		"parameters": {
			"TABLE_NAME": {
				"type": "string",
				"defaultValue": "Date_TEST_TBD"
			},
			"DATE_VALUE": {
				"type": "string",
				"defaultValue": "2019-05-20"
			},
			"DATE_COLUMN_NAME": {
				"type": "string",
				"defaultValue": "AuditDate"
			}
		},
		"folder": {
			"name": "PT R&D"
		}
	}
}